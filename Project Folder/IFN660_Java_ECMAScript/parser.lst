%namespace IFN660_Java_ECMAScript
%union
{
    public int num;
    public string name;
}

%token <num> NUMBER
%token <name> IDENT
%token IF ELSE INT BOOL
%token ARGS
%token MAIN

%left '='
%nonassoc '<'
%left '+'

